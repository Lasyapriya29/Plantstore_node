<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE-edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Online Plant Store</title>
        <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
        <link rel="stylesheet" href="style.css">
    </head>
    <body>
        <%- include('base'); -%>
        <section class="products" id="products">

            <h1 class="heading"><span> latest </span>products </h1>
        
            <div class="box-container">
        
                <div class="box">
                    <div data-id="7" class="image">
                        <img src="images/plant1.jpg" alt="">
                        <div class="content">
                            <h3>Plant</h3>
                            <p>Devi Sridevi Nursery</p>
                            <div class="price"> 150/- </div>
                        </div>
                        <div class="icons">
                            <a href="#" class="fas fa-heart"></a>
                            <a href class="add-to-cart-btn">Add to cart</a>
                            <a href="#" class="fas fa-share"></a>
                        </div>
                    </div>
                </div>
        
                <div class="box">
                    <div data-id="8" class="image">
                        <img src="images/flwr1.jpg" alt="">
                        <div class="content">
                            <h3>flowers</h3>
                            <p>My Green House</p>
                            <div class="price"> 200/- </div>
                        </div>
                        <div class="icons">
                            <a href="#" class="fas fa-heart"></a>
                            <a href class="add-to-cart-btn">Add to cart</a>
                            <a href="#" class="fas fa-share"></a>
                        </div>
                    </div>
                    
                </div>
        
                <div class="box">
                    <div data-id="9" class="image">
                        <img src="images/rose1.jpg" alt="">
                        <div class="content">
                            <h3>rose flowers</h3>
                            <p>Rama Krishna Nursery</p>
                            <div class="price"> 100/- </div>
                        </div>
                        <div class="icons">
                            <a href="#" class="fas fa-heart"></a>
                            <a href class="add-to-cart-btn">Add to cart</a>
                            <a href="#" class="fas fa-share"></a>
                        </div>
                    </div>
                    
                </div>
        
                <div class="box">
                    <div data-id="10" class="image">
                        <img src="images/sunflower.jpg" alt="">
                        <div class="content">
                            <h3>sun flowers</h3>
                            <p>Gardening Shop</p>
                            <div class="price"> 149/-</div>
                        </div>
                        <div class="icons">
                            <a href="#" class="fas fa-heart"></a>
                            <a href class="add-to-cart-btn">Add to cart</a>
                            <a href="#" class="fas fa-share"></a>
                        </div>
                    </div>
                    
                </div>
        
                <div class="box">
                    <div data-id="11" class="image">
                        <img src="images/bouquet1.jpg" alt="">
                        <div class="content">
                            <h3>Bouquet</h3>
                            <p>My Green House</p>
                            <div class="price"> 400/- </div>
                        </div>
                        <div class="icons">
                            <a href="#" class="fas fa-heart"></a>
                            <a href class="add-to-cart-btn">Add to cart</a>
                            <a href="#" class="fas fa-share"></a>
                        </div>
                    </div>
                   
                </div>
        
                <div class="box">
                    <div data-id="12" class="image">
                        <img src="images/img-7.jpg" alt="">
                        <div class="content">
                            <h3>flowers</h3>
                            <p>Keerthana Olive Gardens</p>
                            <div class="price"> 250/- </div>
                        </div>
                        <div class="icons">
                            <a href="#" class="fas fa-heart"></a>
                            <a href class="add-to-cart-btn">Add to cart</a>
                            <a href="#" class="fas fa-share"></a>
                        </div>
                    </div>
                    
                </div>             
        
                        
            </div>
        
        </section> 

        <%- include('footer'); -%>  
</body>
</html>